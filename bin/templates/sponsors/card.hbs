{{#each this}}
<div id="item-{{@index}}"
    class='grid-item {{#if bio}}org-bio {{/if}}{{#if exhibitor}}exhibitor {{/if}}{{createClass type}} {{createClass name}}'
    {{#if bio}}  
    href="#item-{{@index}}"
    data-modal='{{@index}}'
    {{/if}}
>
    <div class='f-cat active'>
        <div class='city-photo-container'>
            <!--picture>
               <source srcset="{{webp img}}" type="image/webp"/-->
               <img alt="" class="city-photo" src="{{img}}" loading="lazy" />
            <!--/picture-->
        </div>
        <div class='profile-details'>
            <h6 class='name'>{{name}}</h6>
        </div>
        <div class='sponsor-details'>
            <h6 class='sponsor-type'>{{type}}</h6>
        </div>
        <div class='icons'>
        {{#if linkedIn}}
            <a class='cat-linkedin' target='_blank' href='{{linkedIn}}' rel='noopener'>
                <span class='icon linkedin' ></span>
            </a>
        {{/if}}
        </div>
    </div>
</div>
{{/each}}
{
	"type":"FeatureCollection","name":"locations",
	"features":[
	{{#each this}}
		{
			"type":"Feature",
			"properties":{
				"type": "location",
				"name": "{{name}}",
				"city": "{{city}}",
				"html": [
					"<div>{{#if img}}<div class='gridElementLogoContainer'><picture><source srcset='{{webp img}}' type='image/webp'/><img class='gridElementLogo' src='{{img}}' loading='lazy'/></picture></div>{{/if}}</div>",
					"<div class='label-location' style='display: flex;flex-direction: row;'><h3>{{city}}</h3>{{#if country}}<img alt='{{country}}' class='flag' src='{{flagUrl}}' style='margin-left: auto;'/>{{/if}}</div>",
					"<div class='ihub-name'>{{name}}</div>",
					"<div class='ihub-type'>{{type}}</div>"{{#if (gt domain.length 0)}},
                	"<div class='chip-domain'><ul class='chips'>{{#each domain}}<li>{{this}}</li>{{/each}}</ul><span class='chips-gradient'></span></div>"
            		{{/if}}{{#if website}},
					"<div class='icons'>{{#if website}}<a class='cat-website' target='_blank' href='{{website}}' rel='noopener'><span class='icon website'></span>{{/if}}</div>"
					{{/if}}
				]
			},
			"geometry":{
				"type":"Point",
				"coordinates":[{{longitude}},{{latitude}}]
			}
		}{{#unless @last}},{{/unless}}
		{{/each}}
	]
}
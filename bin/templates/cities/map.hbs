{
	"type":"FeatureCollection","name":"cities",
	"features":[
	{{#each this}}
		{
			"type":"Feature",
			"properties":{
				"type": "city",
				"city": "{{city}}",
				"html": [
					"<div>{{#if image}}<img class='gridElementLogo' src='{{thumbnail}}'/>{{/if}}</div>",
					"<div class='city-name'><h3>{{city}}</h3>{{#if cityWebsite}}<a class='cat-website' target='_blank' href='{{cityWebsite}}' rel='noopener' title='City Strategy'><span class='icon website'></span></a>{{/if}}</div><div class='label-location' style='display: flex;flex-direction: row;'>{{#if country}}<h6>{{country}}</h6><img alt='{{country}}' class='flag' src='{{flagUrl}}' style='margin-left: auto;'/>{{/if}}</div>"{{#if title1}},
					"<div class='solution-container'><div class='solution-item'><div class='solution-title'>{{title1}}</div><div class='icons'>{{#if website1}}<a class='cat-website' target='_blank' href='{{website1}}' rel='noopener' title='Product Website'><span class='icon website'></span></a>{{/if}}{{#if marketplace1}}<a class='cat-website' target='_blank' href='{{marketplace1}}' title='FIWARE Showcase'><span class='icon marketplace'></span></a>{{/if}}{{#if story1}}<a class='cat-website' target='_blank' href='{{story1}}' title='Impact Story'><span class='icon impactstory'></span></a>{{/if}}</div></div>{{#if title2}}<div class='solution-item'><div class='solution-title'>{{title2}}</div><div class='icons'>{{#if website2}}<a class='cat-website' target='_blank' href='{{website2}}' rel='noopener'><span class='icon website' title='Product Website'></span></a>{{/if}}{{#if marketplace2}}<a class='cat-website' target='_blank' href='{{marketplace2}}' title='FIWARE Showcase'><span class='icon marketplace'></span></a>{{/if}}{{#if story2}}<a class='cat-website' target='_blank' href='{{story2}}' title='Impact Story'><span class='icon impactstory'></span></a>{{/if}}</div></div>{{/if}}</div>"{{/if}}
				]
			},
			"geometry":{
				"type":"Point",
				"coordinates":[{{lng}},{{lat}}]
			}
		}{{#unless @last}},{{/unless}}
		{{/each}}
	]
}
{{#each this}}
    <div id="enabler-{{createClass name}}" class='grid-item {{createClass type}} {{createClasses domain}} {{createClasses technology}} {{#if (eq member true)}}member{{/if}}'>
        {{{createAnchor .. type @index}}}
        <div class='gridElementTextContainer'>
            <div class='label-type'>
                {{{badge}}}
            </div>
            <p class='cat-name'>{{name}}</p>
            <div class="details-container">
                <div class='label-summary'>
                    <span class='cat-excerpt'>{{summary}}</span>
                </div>
            </div>
        </div>
        <div class='card-attributes'>
            {{#if (gt technology.length 0)}}
            <div class='chip-technology'>
                <p class="label-card">Technology</p>
                <ul class='chips'>{{#each technology}}<li>{{this}}</li>{{/each}}</ul>
                <span class='chips-gradient'></span>
            </div>
            {{/if}}
            {{#if (gt domain.length 0)}}
            <div class='chip-domain'>
                <p class="label-card">Function</p>
                <ul class='chips'>{{#each domain}}<li>{{this}}</li>{{/each}}</ul>
                <span class='chips-gradient'></span>
            </div>
            {{/if}}
        </div>
        <div class='foot'>
            {{#if (eq member true)}}
            <div class='cat-info' data-modal='{{math @index "+" 6}}'>
                <span class="material-symbols-outlined">info</span>
                More Info
            </div>
            {{/if}}
            {{#if (eq member false)}}
            <span> </span>
            {{/if}}
            <a class='cat-details-primary' target="_blank" href="{{documentation}}">
                <span class='material-symbols-outlined'>description</span>
                Docs
            </a>
        </div>
    </div>
{{/each}}
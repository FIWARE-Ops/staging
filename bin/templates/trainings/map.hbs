{
	"type":"FeatureCollection","name":"trainings",
	"features":[
	{{#each this}}
		{
			"type":"Feature",
			"properties":{
				"name": "{{name}}",
				"city": "{{city}}",
				"html": [
					"<div>{{#if image}}<div class='gridElementLogoContainer'><picture><source srcset='{{webp img}}' type='image/webp'/><img class='gridElementLogo' src='{{img}}' loading='lazy'/></picture></div>{{/if}}</div>",
					"<div class='label-location' style='display: flex;flex-direction: row;'><h3>{{city}}</h3>{{#if country}}<img alt='{{country}}' class='flag' src='{{flagUrl}}' style='margin-left: auto;'/>{{/if}}</div>",
					"<div class='ihub-name'>{{name}}</div>"
				]
			},
			"geometry":{
				"type":"Point",
				"coordinates":[{{longitude}},{{latitude}}]
			}
		}{{#unless @last}},{{/unless}}
		{{/each}}
	]
}
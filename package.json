    "got": "~11.8.5",
    "handlebars": "^4.7.8",
    "image-downloader": "^4.3.0",
    "image-size": "^1.1.1",
    "jsdom": "^24.1.0",
    "json-schema-ref-parser": "^9.0.1",
    "markdown-to-txt": "^2.0.1",
    "node-font2base64": "^0.9.2",
    "node-html-to-image": "5.0.0",
    "prettier": "3.3.2",
    "qrcode": "^1.5.4",
    "sharp": "^0.33.5",
    "showdown": "^2.1.0",
    "underscore": "^1.13.6",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "eslint": "^9.6.0",
    "eslint-config-tamia": "^8.1.3",
    "eslint-plugin-prettier": "^5.1.3"
  },
